{
  "name": "xstate-redux",
  "version": "1.0.0",
  "description": "Redux middleware/reducer to use xstate with redux",
  "engines": {
    "node": ">= 12.13 <13"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "github:go-aos/xstate-redux",
  "homepage": "https://github.com/go-aos/xstate-redux/blob/master/README.md",
  "bugs": "https://github.com/go-aos/xstate-redux/issues",
  "license": "MIT",
  "keywords": [
    "fsm",
    "redux",
    "stateCharts",
    "xstate"
  ],
  "authors": [
    "Stephane Barale <stephane.barale@go-aos.io> (https://github.com/C0r3y8)",
    "AOS"
  ],
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "build": "tsc --build tsconfig.release.json",
    "build:watch": "tsc --build tsconfig.release.json --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "postinstall": "npm run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "tsutils": "^3.17.1",
    "typescript": "^3.8.3",
    "xstate": "^4.9.1"
  },
  "peerDependencies": {
    "redux": "^3.7.2 || ^4.0.0",
    "xstate": "^3.1.0 || ^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run lint && npm run test"
    }
  }
}
