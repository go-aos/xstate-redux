{
  "name": "xstate-redux",
  "version": "0.7.0",
  "description": "Redux middleware/reducer to use xstate with redux",
  "repository": "github:go-aos/xstate-redux",
  "bugs": "https://github.com/go-aos/xstate-redux/issues",
  "main": "lib/xstate-redux.js",
  "module": "es/xstate-redux.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/xstate-redux.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/xstate-redux.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/xstate-redux.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/xstate-redux.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist es",
    "prepare": "npm run clean && npm test && npm run build",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [
    "fsm",
    "redux",
    "stateCharts",
    "xstate"
  ],
  "authors": [
    "Stephane Barale <stephane.barale@go-aos.io> (https://github.com/C0r3y8)",
    "AOS"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.6.0",
    "jest-runner-eslint": "^0.7.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.3",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-uglify": "^6.0.2",
    "redux": "^4.0.1",
    "xstate": "^4.4.0"
  },
  "peerDependencies": {
    "redux": "^3.7.2 || ^4.0.0",
    "xstate": "^3.1.0 || ^4.0.0"
  },
  "npmName": "xstate-redux",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
